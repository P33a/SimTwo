<?xml version="1.0" ?>
<obstacles>
   <defines>
    <const name='base_height' value='0.015' />
    <const name='cell_width' value='0.180' />  
    <const name='cell_depth' value='0.120' /> 
    <const name='wall_height' value='0.120' />
    <const name='wall_thickness'  value='0.010' />
    <const name='console_height' value='0.04' />
    <const name='console_width' value='0.12' />  
    <const name='console_depth' value='0.08' /> 
  </defines>

  <cuboid>
    <ID value='base'/>
    <size x='5*cell_width' y='cell_depth' z='base_height'/>
    <pos x='0' y='0' z='base_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='64' g='64' b='64'/>
  </cuboid>

  <cuboid>
    <ID value='right_wall'/>
    <size x='wall_thickness' y='cell_depth' z='wall_height'/>
    <pos x='5*cell_width/2 - wall_thickness/2' y='0' z='base_height + wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='240' g='240' b='240'/>
  </cuboid>

  <cuboid>
    <ID value='inner_wall_1'/>
    <size x='wall_thickness' y='cell_depth' z='wall_height'/>
    <pos x='3*cell_width/2' y='0' z='base_height + wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='240' g='240' b='240'/>
  </cuboid>

  <cuboid>
    <ID value='inner_wall_2'/>
    <size x='wall_thickness' y='cell_depth' z='wall_height'/>
    <pos x='1*cell_width/2' y='0' z='base_height + wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='240' g='240' b='240'/>
  </cuboid>

  <cuboid>
    <ID value='inner_wall_3'/>
    <size x='wall_thickness' y='cell_depth' z='wall_height'/>
    <pos x='-1*cell_width/2' y='0' z='base_height + wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='240' g='240' b='240'/>
  </cuboid>

  <cuboid>
    <ID value='inner_wall_4'/>
    <size x='wall_thickness' y='cell_depth' z='wall_height'/>
    <pos x='-3*cell_width/2' y='0' z='base_height + wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='240' g='240' b='240'/>
  </cuboid>

  <cuboid>
    <ID value='left_wall'/>
    <size x='wall_thickness' y='cell_depth' z='wall_height'/>
    <pos x='-5*cell_width/2 + wall_thickness/2' y='0' z='base_height + wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='240' g='240' b='240'/>
  </cuboid>

  <cuboid>
    <ID value='back_wall'/>
    <size x='5*cell_width' y='wall_thickness' z='wall_height'/>
    <pos x='0' y='cell_depth/2 - wall_thickness/2' z='base_height + wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='255' g='255' b='255'/>
  </cuboid>

  <cuboid>
    <ID value='ceiling'/>
    <size x='5*cell_width' y='cell_depth' z='wall_thickness'/>
    <pos x='0' y='0' z='base_height + wall_height + wall_thickness/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='128' g='128' b='128'/>
  </cuboid>

  <cuboid>
    <ID value='console'/>
    <size x='5*cell_width' y='console_depth' z='console_height'/>
    <pos x='0' y='-(cell_depth - console_depth)/2' z='base_height + wall_height + wall_thickness + console_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='200' g='200' b='200'/>
  </cuboid>


</obstacles>
