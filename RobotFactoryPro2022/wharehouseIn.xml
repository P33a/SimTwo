<?xml version="1.0" ?>
<obstacles>
   <defines>
    <const name='base_height' value='0.002' />

    <const name='cell_x' value='0.15' />
    <const name='cell_y' value='0.08' />
    
    <const name='wall_height' value='0.065' />
    <const name='wall_thickness'  value='0.02' />    
    <const name='marker_size' value='0.055' />
    <const name='marker_thickness' value='0.001' />
  </defines>
   

  <!-- Walls -->
  <!-- right -->
  <cuboid>
    <ID value='right_wall'/>
    <size x='wall_thickness' y='cell_y' z='wall_height'/>
    <pos x='-2 * cell_x' y='0' z='wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='30' g='144' b='255'/>
  </cuboid>
  <!-- left -->                
  <cuboid>
    <ID value='left_wall'/>
    <size x='wall_thickness' y='cell_y' z='wall_height'/>
    <pos x='2 * cell_x' y='0' z='wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='30' g='144' b='255'/>
  </cuboid>
  <!-- back -->
  <cuboid>
    <ID value='back_wall'/>
    <size x='4 * cell_x + wall_thickness' y='wall_thickness' z='wall_height'/>
    <pos x='0' y='cell_y/2' z='base_height + wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='30' g='144' b='255'/>
  </cuboid>

  <cuboid>
    <ID value='back_wall_marker'/>
    <size x='4 * cell_x + wall_thickness' y='wall_thickness' z='wall_height'/>
    <pos x='0' y='cell_y/2' z='base_height + 1.5 * wall_height'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='30' g='144' b='255'/>
  </cuboid>

  <cuboid>
    <ID value='back_wall_marker_1'/>
    <size x='marker_size' y='marker_size' z='marker_thickness'/>
    <pos x='cell_x/2 - 2 * cell_x' y='cell_y/2 - wall_thickness/2 - marker_thickness/2' z='base_height + 1.5 * wall_height'/>
    <rot_deg x='90' y='0' z='0'/>
    <color_rgb r='255' g='255' b='255'/>
    <texture name='5x5_1000-50.png' scale='20'/>
  </cuboid>

  <cuboid>
    <ID value='back_wall_marker_2'/>
    <size x='marker_size' y='marker_size' z='marker_thickness'/>
    <pos x='cell_x/2 - 1 * cell_x' y='cell_y/2 - wall_thickness/2 - marker_thickness/2' z='base_height + 1.5 * wall_height'/>
    <rot_deg x='90' y='0' z='0'/>
    <color_rgb r='255' g='255' b='255'/>
    <texture name='5x5_1000-51.png' scale='20'/>
  </cuboid>

  <cuboid>
    <ID value='back_wall_marker_3'/>
    <size x='marker_size' y='marker_size' z='marker_thickness'/>
    <pos x='cell_x/2 - 0 * cell_x' y='cell_y/2 - wall_thickness/2 - marker_thickness/2' z='base_height + 1.5 * wall_height'/>
    <rot_deg x='90' y='0' z='0'/>
    <color_rgb r='255' g='255' b='255'/>
    <texture name='5x5_1000-52.png' scale='20'/>
  </cuboid>

  <cuboid>
    <ID value='back_wall_marker_4'/>
    <size x='marker_size' y='marker_size' z='marker_thickness'/>
    <pos x='cell_x/2 + 1 * cell_x' y='cell_y/2 - wall_thickness/2 - marker_thickness/2' z='base_height + 1.5 * wall_height'/>
    <rot_deg x='90' y='0' z='0'/>
    <color_rgb r='255' g='255' b='255'/>
    <texture name='5x5_1000-53.png' scale='20'/>
  </cuboid>

  <!-- Inside -->
  <!-- center wall -->                
  <cuboid>
    <ID value='inside_wall_1'/>
    <size x='wall_thickness' y='cell_y' z='wall_height'/>
    <pos x='-1 * cell_x' y='0' z='wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='30' g='144' b='255'/>
  </cuboid>
  <!-- center/right wall -->
  <cuboid>
    <ID value='inside_wall_2'/>
    <size x='wall_thickness' y='cell_y' z='wall_height'/>
    <pos x='0' y='0' z='wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='30' g='144' b='255'/>
  </cuboid>
  <!-- center/left wall -->
  <cuboid>
    <ID value='inside_wall_3'/>
    <size x='wall_thickness' y='cell_y' z='wall_height'/>
    <pos x='1 * cell_x' y='0' z='wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='30' g='144' b='255'/>
  </cuboid>

                  
</obstacles>
